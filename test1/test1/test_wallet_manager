class WalletManager(object):
   def __init__(self):

   def getbalance(self, address):
       if address == 'aaaabbbbcccc':
          return 2000.0
       else if address == 'bbbbccccdddd':
          return 3000.0

  def transfer(self, fromaddress, toaddress, amount):
      print "transfer %f from %s to %s" % (amount, fromaddress, toaddress)
