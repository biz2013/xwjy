[Unit]
Description=coin exchange gunicorn daemon
After=network.target

[Service]
User=chi
Group=www-data
WorkingDirectory=/home/chi/workspace/python/projects/xwjy/coinExchange
ExecStart=/home/chi/workspace/python/projects/xwjy/dj2env/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/chi/workspace/python/projects/xwjy/coinExchange/coinExchange_nginx.sock coinExchange.wsgi:application

[Install]
WantedBy=multi-user.target