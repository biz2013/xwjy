[Unit]
Description=coin exchange gunicorn daemon
After=network.target

[Service]
User=root
Group=www-data
WorkingDirectory=/home/osboxes/workspace/xwjy/coinExchange
ExecStart=/home/osboxes/workspace/xwjy/coinExchange/dj2env/bin/gunicorn --workers 3 --bind unix:/home/osboxes/workspace/xwjy/coinExchange/coinExchange_nginx.sock coinExchange.wsgi:application --access-logfile /var/log/gunicorn/web.access.log --error-logfile /var/log/gunicorn/web.error.log 

[Install]
WantedBy=multi-user.target
