server {
    listen 4455;
    #server_name server_domain_or_IP;
    server_name 127.0.0.1;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        #root /home/sammy/myproject;
        root /home/chi/workspace/python/projects/xwjy/coinExchange;
    }

    location /media/ {
        root /home/chi/workspace/python/projects/xwjy/coinExchange;
    }

    location / {
        include proxy_params;
        #proxy_pass http://unix:/home/sammy/myproject/myproject.sock;
        proxy_pass http://unix:/home/chi/workspace/python/projects/xwjy/coinExchange/coinExchange_nginx.sock;
    }
}